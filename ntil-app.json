{
    "id": "ntil",
    "instances": 1,
    "cpus": 1,
    "mem": 512,
    "user": "vagrant",
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "libmesos/ubuntu",
            "network": "BRIDGE",
            "portMappings": [{
                "containerPort": 9889,
                "protocol": "tcp"
